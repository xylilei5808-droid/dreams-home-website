{
  "version": 2,
  "name": "dreams-home-website",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NOTION_TOKEN": "@notion-token",
    "NOTION_PAGE_ID": "@notion-page-id",
    "NOTION_ROOMS_DB_ID": "@notion-rooms-db-id",
    "NOTION_JOURNAL_DB_ID": "@notion-journal-db-id",
    "NOTION_PLANS_DB_ID": "@notion-plans-db-id",
    "NOTION_STORIES_DB_ID": "@notion-stories-db-id",
    "NOTION_EXPERIENCES_DB_ID": "@notion-experiences-db-id",
    "NOTION_CUISINES_DB_ID": "@notion-cuisines-db-id",
    "NOTION_DISHES_DB_ID": "@notion-dishes-db-id",
    "NOTION_CHEFS_DB_ID": "@notion-chefs-db-id"
  },
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  }
}
